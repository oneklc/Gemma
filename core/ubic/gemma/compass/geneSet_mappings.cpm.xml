<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 2.2//EN"
    "http://www.compass-project.org/dtd/compass-core-mapping-2.2.dtd">

	<!--
		This document is a Compass mapping file, allowing the Gemma objects to be indexed and searched. All "searchable"
		attributes (as well as associations) should be included here. author: pp version: $Id: geneSet_mappings.cpm.xml,v 1.1 2010/06/03 15:07:38 paul Exp $
	-->
<compass-core-mapping package="ubic.gemma.model">
	<class name="genome.gene.GeneSetImpl" alias="geneSet" root="true">
		<id name="id" />
		<property name="name">
			<meta-data>name</meta-data>
			<meta-data>gene set name</meta-data>
		</property>
		<property name="description">
			<meta-data>description</meta-data>
			<meta-data>gene set description</meta-data>
		</property>
	</class>
</compass-core-mapping>